#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules/.pnpm/terser@4.7.0/node_modules/terser/bin/terser/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules/.pnpm/terser@4.7.0/node_modules/terser/bin/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules/.pnpm/terser@4.7.0/node_modules/terser/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules/.pnpm/terser@4.7.0/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules/.pnpm/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/sandbox-basement/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/academind - svelte-sapper complete course/node_modules:/Users/fabezio/Sandbox/dev/web/svelte/node_modules:/Users/fabezio/Sandbox/dev/web/node_modules:/Users/fabezio/Sandbox/dev/node_modules:/Users/fabezio/Sandbox/node_modules:/Users/fabezio/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@4.7.0/node_modules/terser/bin/terser" "$@"
else 
  exec node  "$basedir/../../../../../terser@4.7.0/node_modules/terser/bin/terser" "$@"
fi
